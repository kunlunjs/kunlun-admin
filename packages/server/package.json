    "@types/bcrypt": "^5.0.0",
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
