{
  "name": "@kunlun-admin/server",
  "version": "0.0.1",
  "description": "",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prestart:prod": "npm run build",
    "start:prod": "node dist/main",
    "prestart:pm2": "npm run build",
    "start:pm2": "pm2 start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "lint:generate-nestjs": "eslint --config \"../../.eslintrc.js\" \"src/{@generated,modules}/**/*.ts\" --fix --cache",
    "format:generate-nestjs": "prettier --config \"../../.prettierrc.js\" --write \"src/{@generated,modules}/**/*.ts\"",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "migrate:resolve": "prisma migrate resolve --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate && npm run lint:generate-nestjs && npm run format:generate-nestjs ",
    "prisma:generate:watch": "prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate && npm run seed",
    "seed": "prisma db seed",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "postinstall": "echo 'skip auto run \"npm run prisma:generate\"'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
